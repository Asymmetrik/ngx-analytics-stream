{
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-console": 0,
    "no-debugger": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-unused-expressions": 2,
    "no-with": 2,
    "dot-notation": [2, { "allowKeywords": true }],
    "no-extra-parens": [2, "functions"],
    "new-parens": 2,
    "semi": 2,
		"no-unused-vars": 0,
    "semi-spacing": [2, {"before": false, "after": true}],
    "space-unary-ops": [2, { "words": true, "nonwords": false }]
  }
}
